{
  "name": "ass-js",
  "description": "Assembler.js",
  "version": "2.0.1",
  "keywords": [
    "x86",
    "x64",
    "x86_64",
    "arm",
    "arm64",
    "native",
    "native-code",
    "machine-code",
    "asm",
    "compiler",
    "assembler",
    "compiler",
    "intel",
    "amd",
    "nasm",
    "gasm",
    "ethereum",
    "bitcoin",
    "wasm",
    "ethereum-vm",
    "vm",
    "blockchain"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/streamich/ass-js"
  },
  "main": "lib/index.js",
  "dependencies": {
    "fast-extend": "^0.0.2"
  },
  "devDependencies": {
    "jest": "21.2.1",
    "jest-tap-reporter": "1.9.0",
    "ts-jest": "21.2.4",
    "mocha": "3.4.2",
    "chai": "4.1.2",
    "typescript": "2.4.2",
    "ts-node": "3.3.0",
    "gulp": "3.9.1",
    "gulp-typescript": "3.2.1",
    "source-map-support": "0.4.15",
    "mkdirp": "0.5.1",
    "coffee-register": "0.1.3",
    "static-buffer": "1.0.1",
    "rimraf": "2.6.2",
    "ethereumjs-vm": "2.3.2",
    "semantic-release": "11.2.0",
    "@types/node": "8.5.5",
    "@types/jest": "20.0.8"
  },
  "scripts": {
    "build": "npm run clean && npm run build-ts",
    "build-ts": "gulp build-ts",
    "clean": "rimraf lib/",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "semantic-release": "semantic-release"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": ".*/__tests__/.*\\.(test|spec)\\.(jsx?|tsx?)$",
    "reporters": [
      "jest-tap-reporter"
    ]
  }
}
